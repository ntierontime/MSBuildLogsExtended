@{
    ViewBag.Title = Framework.Resources.UIStringResource.ImportAlternativeText;
}
@section featured {
    <hgroup>
        <h2>@ViewBag.Title.</h2>
        <h3>@Framework.Resources.UIStringResource.Data_Import_DescriptionText</h3>
        <h4>@ViewBag.Message</h4>
    </hgroup>
}
@using (Html.BeginForm("Import", "BuildEventCode", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <div class="btn-group">
        <input type="file" name="file" id="file" />
        <input class="btn btn-default" role="button" type="submit" name="Import" value="@Framework.Resources.UIStringResource.ImportAlternativeText" />
    </div>
}
