@using UrlStringsAlias = MSBuildLogsExtended.AspNetMvc40Rasor.Helpers.UrlStrings;
<div class="row">
    <div class="col-sm-12 col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
				<h2>@ViewBag.Title</h2>
				<h4>@Framework.Resources.UIStringResource.Data_Import_DescriptionText</h4>
				<h3>@ViewBag.Message</h3>
            </div>
            <div class="panel-body">
@using (Html.BeginForm(UrlStringsAlias.ActionName_BuildEventCode_Import, UrlStringsAlias.ControllerName_BuildEventCode, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
	<div>
		<input type="file" name="file" id="file" />
			<div class="col-xs-12 col-sm-12 col-md-5 col-lg-5 pull-right nopadding">
            <div class="btn-group btn-group-justified" role="group" aria-label="...">
                <div class="btn-group">
					<button class="btn btn-primary" role="button" type="submit">
						<span class="glyphicon glyphicon-import"></span>
						<span class="submit-text hidden-xs hidden-sm visible-md-inline visible-lg-inline">@Framework.Resources.UIStringResource.ImportAlternativeText</span>
					</button>
                </div>
                <div class="btn-group">
					<a href="@Url.Action(UrlStringsAlias.ActionName_BuildEventCode_Index)" class="btn btn-default" role="button" type="button">
						<span class="glyphicon glyphicon-ban-circle"></span><span class="hidden-xs hidden-sm visible-md-inline visible-lg-inline">@Framework.Resources.UIStringResource.BackAlternativeText</span>
					</a>
                </div>
            </div>
        </div>
	</div>
}
            </div>
        </div>
    </div>
</div>
