@using UrlStringsAlias = MSBuildLogsExtended.AspNetMvc50Rasor.Helpers.UrlStrings;
@model MSBuildLogsExtended.AspNetMvc40ViewModel.BuildItemVM
@section featured {
    <hgroup>
        <h2>@Model.ContentData.Title.</h2>
        <h4>@Model.ContentData.Summary</h4>
    </hgroup>
}
@if (Model.StatusOfResult != Framework.CommonBLLEntities.BusinessLogicLayerResponseStatus.MessageOK && Model.StatusOfResult != Framework.CommonBLLEntities.BusinessLogicLayerResponseStatus.UIProcessReady)
{
	<div class="text-danger">@Model.StatusMessageOfResult</div>
}
	<legend>@MSBuildLogsExtended.Resources.UIStringResourcePerEntityBuild.Build.</legend>
<div class="form-group form-inline">
			@Html.LabelFor(model => model.Item.Solution_1_Name, new { @class = "control-label" })
        <div class="form-control">
@Html.ActionLink(Model.Item.Solution_1_Name, UrlStringsAlias.ActionName_Solution_WPEntityRelatedOfSolution, UrlStringsAlias.ControllerName_Solution, new { isToCompareIdByIdentifierOftOfByIdentifier=true, valueToCompareIdByIdentifierOftOfByIdentifier = Model.Item.SolutionId }, null)
        </div>
    </div>
@Html.HiddenFor(model => model.Item.Id)@Html.HiddenFor(model => model.Item.SolutionId)  <div class="form-group form-inline">
			@Html.LabelFor(model => model.Item.Name, new { @class = "control-label" })
        <div class="form-control">
			@Html.DisplayFor(model => model.Item.Name)
        </div>
    </div>
  <div class="form-group form-inline">
			@Html.LabelFor(model => model.Item.Description, new { @class = "control-label" })
        <div class="form-control">
			@Html.DisplayFor(model => model.Item.Description)
        </div>
    </div>
  <div class="form-group form-inline">
			@Html.LabelFor(model => model.Item.BuildStartTime, new { @class = "control-label" })
        <div class="form-control">
			@Html.DisplayFor(model => model.Item.BuildStartTime)
        </div>
    </div>
    <div class="btn-group" role="group">
        <input class="btn btn-default" role="button" type="button" value="@Framework.Resources.UIStringResource.EditAlternativeText" onclick="location.href='@Url.Action(UrlStringsAlias.ActionName_Build_Edit, new { isToCompareIdByIdentifierOftOfByIdentifier=true, valueToCompareIdByIdentifierOftOfByIdentifier = Model.Item.Id })'" />
        <input class="btn btn-default" role="button" type="button" value="@Framework.Resources.UIStringResource.DeleteAlternativeText" onclick="location.href='@Url.Action(UrlStringsAlias.ActionName_Build_Delete, new { isToCompareIdByIdentifierOftOfByIdentifier=true, valueToCompareIdByIdentifierOftOfByIdentifier = Model.Item.Id })'" />
        <input class="btn btn-default" role="button" type="button" value="@Framework.Resources.UIStringResource.BackAlternativeText" onclick="location.href='@Url.Action(UrlStringsAlias.ActionName_Build_Index)'" />
        <input class="btn btn-default" role="button" type="button" value="@Framework.Resources.UIStringResource.CopyAlternativeText" onclick="location.href='@Url.Action(UrlStringsAlias.ActionName_Build_Copy, new { isToCompareIdByIdentifierOftOfByIdentifier=true, valueToCompareIdByIdentifierOftOfByIdentifier = Model.Item.Id })'" />
    </div>
